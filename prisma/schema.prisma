generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:pass@localhost:5432/?schema=public"
}

model Build {
  id        Int    @id @default(autoincrement())
  gpu       String
  cpu       String
  cooler    String
  ram       String
  numram    Int
  drive     String
  numstorage Int
  mobo      String
  psu       String
}
